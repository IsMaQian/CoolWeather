    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/textViewaphard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#1F1F1F"
        android:text="HardVesion"/>

    <TextView
        android:id="@+id/textViewapsoft"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:textColor="#1F1F1F"
        android:text="SoftVesion"/>

    <TextView
        android:id="@+id/textViewapid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:textColor="#1F1F1F"
        android:text="IdVesion"/>

		</LinearLayout>
	</LinearLayout>
	 	<View
