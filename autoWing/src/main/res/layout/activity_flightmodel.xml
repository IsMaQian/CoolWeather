<?xml version="1.0" encoding="utf-8"?>
<com.example.view.SildingFinishLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/sildingFinishLayoutmodel"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#686868" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/RelativeLayoutmodel0"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="0dp" >

            <TextView
                android:id="@+id/textViewledset"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="5dp"
                android:text="灯光设置"
                android:textColor="#f1f1f1"
                android:textSize="20sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RelativeLayout001"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/RelativeLayoutmodel0"
            android:clickable="false"
            android:focusable="false" >

            <Button
                android:id="@+id/buttonmodelselect"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/selector"
                android:gravity="center_vertical|left"
                android:text="机型选择"
                android:textColor="#f1f1f1"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/textViewmodelset"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@id/buttonmodelselect"
                android:layout_alignBottom="@id/buttonmodelselect"
                android:layout_alignParentRight="true"
                android:layout_marginRight="15dp"
                android:text="四轴叉字逆"
                android:textColor="#f1f1f1"
                android:textSize="14sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RelativeLayout002"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/RelativeLayout001"
            android:clickable="false"
            android:focusable="false" >

            <Button
                android:id="@+id/buttondianchi"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="10dp"
                android:background="@drawable/selector"
                android:gravity="center_vertical|left"
                android:text="电池选型"
                android:textColor="#f1f1f1"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/textViewdianchiset"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@id/buttondianchi"
                android:layout_alignBottom="@id/buttondianchi"
                android:layout_alignRight="@id/buttondianchi"
                android:layout_marginRight="30dp"
                android:text="6S"
                android:textColor="#f1f1f1"
                android:textSize="14sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RelativeLayout003"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/RelativeLayout002"
            android:clickable="false"
            android:focusable="false" >

            <Button
                android:id="@+id/buttonykqselect"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="10dp"
                android:background="@drawable/selector"
                android:gravity="center_vertical|left"
                android:text="遥控选择"
                android:textColor="#f1f1f1"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/textViewykqset"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@id/buttonykqselect"
                android:layout_alignBottom="@id/buttonykqselect"
                android:layout_alignRight="@id/buttonykqselect"
                android:layout_marginRight="30dp"
                android:text="14SG"
                android:textColor="#f1f1f1"
                android:textSize="14sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RelativeLayout004"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/RelativeLayout003"
            android:clickable="false"
            android:focusable="false" >

            <Button
                android:id="@+id/button1"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="10dp"
                android:background="@drawable/selector"
                android:gravity="center_vertical|left"
                android:text="遥控回中"
                android:textColor="#f1f1f1"
                android:textSize="14sp" />

            <Spinner
                android:id="@+id/spinnerhuizhong"
                style="@style/spinnerstyle"
                android:layout_width="60dp"
                android:layout_height="40dp"
                android:layout_alignBottom="@+id/button1"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:layout_marginTop="0dp"
                android:background="@drawable/shapes"
                android:gravity="center_vertical|right"
                android:textColor="#f1f1f1"
                android:textSize="14sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RelativeLayout005"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/RelativeLayout004"
            android:clickable="false"
            android:focusable="false" >

            <Button
                android:id="@+id/button2"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="10dip"
                android:background="@drawable/selector"
                android:gravity="center_vertical|left"
                android:text="怠速设置"
                android:textColor="#f1f1f1"
                android:textSize="14sp" />

            <com.example.autowing.NumberSeekBar
                android:id="@+id/bar0"
                style="@style/NumberProgressBar_Default"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="75dp"
                android:focusable="false"
                android:max="100"
                android:progressDrawable="@drawable/numberseekbar_background"
                android:thumb="@drawable/thumb_bar" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RelativeLayout006"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/RelativeLayout005"
            android:clickable="false"
            android:focusable="false" >

            <Button
                android:id="@+id/vesiontype"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="10dp"
                android:background="@drawable/selector"
                android:gravity="center_vertical|left"
                android:text="版本信息"
                android:textColor="#f1f1f1"
                android:textSize="14sp" />
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/Linear02"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="0dp"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="15dp"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/buttongetjx_btn"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:background="@drawable/selector"
                android:gravity="center_vertical|left"
                android:text="参数获取"
                android:textColor="#f1f1f1"
                android:textSize="14sp" />

            <Button
                android:id="@+id/buttonindexset"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="120dp"
                android:background="@drawable/selector"
                android:gravity="center_vertical|right"
                android:text="参数设置"
                android:textColor="#f1f1f1"
                android:textSize="14sp" />
        </LinearLayout>
    </RelativeLayout>

</com.example.view.SildingFinishLayout>